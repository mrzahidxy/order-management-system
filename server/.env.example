# Database connection string (PostgreSQL)
DATABASE_URL="postgresql://<username>:<password>@<host>:<port>/<database>?schema=<schema>"
# Example: DATABASE_URL="postgresql://postgres:123456@localhost:5432/order-server?schema=public"

# JSON Web Token (JWT) secret key (use a placeholder or instruction for secret)
JWT_SECRET="your-jwt-secret"

# Server port
PORT=8080

